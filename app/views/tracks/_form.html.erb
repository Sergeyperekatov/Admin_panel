<%= render 'shared/errors', object: @track %>
<%= form_with model: @track, local: true do |f| %>
  <div class="form-group">
    <%= f.label :Исполнитель %>
    <%= f.text_field :author, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Название_трэка %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Предпологаемая_дата_релиза %>
    <%= f.date_field :release_date, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :Обложка %>
    <%= f.file_field :image, class: "form-control" %>
    <%= f.label :Удалить_обложку %>
    <%= f.check_box :remove_image %>
  </div>
  <div class="form-group">
    <%= f.label :Аудио %>
    <%= f.file_field :audio, class: "form-control" %>
  </div>
    <%= f.submit class: "btn btn-primary" %>
<% end %>